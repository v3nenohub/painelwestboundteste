    -- SISTEMA DE TOGGLE DO PAINEL
    -- ============================================
    if UserInputService then
        local toggleConnection = UserInputService.InputBegan:Connect(function(input, gameProcessed)
            if not gameProcessed and input.KeyCode == ToggleKey then
                MainFrame.Visible = not MainFrame.Visible
                if isMobile then
                    local mobileBtn = ScreenGui:FindFirstChild("MobileMinimizeButton")
                    if mobileBtn then
                        if MainFrame.Visible then
                            mobileBtn.BackgroundColor3 = Color3.fromRGB(0, 150, 0)
                        else
                            mobileBtn.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
                        end
                    end
                end
                Notify(MainFrame.Visible and "PANEL OPENED" or "PANEL CLOSED")
            end
        end)
        table.insert(ActiveConnections, toggleConnection)
    end

